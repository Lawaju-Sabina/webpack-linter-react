{
    "extends": [
      "airbnb-base",
      "plugin:jsx-a11y/recommended"
    ],
    "env": {
      "browser": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "plugins": [
      "jsx-a11y"
    ],
    "rules": {
      "quotes": "off",
      "semi": ["error", "always"],
      "no-unused-vars": ["warn"],
      "linebreak-style": "off",
      "eol-last": "off"
    }
  }
  